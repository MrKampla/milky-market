{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "compile": {
      "outputs": ["artifacts/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["compile"],
      "cache": false
    }
  },
  "globalEnv": ["POLYGON_MAINNET_RPC", "PRIVATE_KEY", "PRIVATE_KEY_2"]
}
